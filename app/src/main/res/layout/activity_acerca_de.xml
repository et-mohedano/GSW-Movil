<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textAlignment="center"
    tools:context=".acercaDe">
        <ScrollView
            android:id="@+id/ScrollView02AcercaDe"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:ignore="MissingConstraints">



            <RelativeLayout
                android:id="@+id/lytAceHeader"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingHorizontal="20dp"
                android:paddingTop="5dp">

                <ImageView
                    android:id="@+id/imgAceLogo"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:src="@drawable/logogsw" />

                <TextView
                    android:id="@+id/txtAceTitulo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_toEndOf="@id/imgAceLogo"
                    android:gravity="center_horizontal"
                    android:text="GrasShopperWeb"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textSize="20dp" />

            </RelativeLayout>


            <TextView
                android:id="@+id/txtAceLine"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/lytAceHeader"
                android:layout_marginTop="5dp"
                android:background="@color/black"
                android:gravity="center_horizontal"
                android:text="Acerca De"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                android:textColor="@color/white" />


            <TextView
                android:id="@+id/txtDescipcionAcercaDe"
                android:layout_width="139dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtAceLine"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:text="Equipo de desarrollo LIPTech"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <ImageView
                android:id="@+id/imgvwAlar"
                android:layout_width="145dp"
                android:layout_height="117dp"
                android:layout_below="@+id/txtDescipcionAcercaDe"
                android:layout_marginTop="10dp"
                android:layout_marginStart="30dp"
                app:srcCompat="@drawable/alar" />

            <ImageView
                android:id="@+id/imgvwDmmh"
                android:layout_width="145dp"
                android:layout_height="117dp"
                android:layout_below="@+id/txtDescipcionAcercaDe"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="30dp"
                android:layout_marginTop="10dp"
                app:srcCompat="@drawable/dmmh" />

            <TextView
                android:id="@+id/txtNomAn"
                android:layout_width="152dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imgvwAlar"
                android:layout_marginStart="30dp"
                android:layout_marginTop="10dp"
                android:text="Andrea Lizbeth Arriaga Resendis"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <TextView
                android:id="@+id/txtNomD"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imgvwDmmh"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"
                android:text="Dylan Michel Mendoza Hernández"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <TextView
                android:id="@+id/txtMatriculaA"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtNomAn"
                android:layout_marginTop="10dp"
                android:text="183111323"
                android:textAlignment="center"
                android:layout_marginStart="30dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="166dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtNomD"
                android:layout_marginTop="10dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="30dp"
                android:text="183110210"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <ImageView
                android:id="@+id/imgvwEmt"
                android:layout_width="145dp"
                android:layout_height="117dp"
                android:layout_below="@+id/txtMatriculaA"
                android:layout_marginTop="10dp"
                android:layout_marginStart="30dp"
                app:srcCompat="@drawable/emt" />

            <TextView
                android:id="@+id/txtNomEf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imgvwEmt"
                android:layout_marginStart="30dp"
                android:layout_marginTop="10dp"
                android:text="Efraín Mohedano Torres"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <TextView
                android:id="@+id/txtNomMa"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView3"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"
                android:text="Marco Antonio Vega Parra"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <TextView
                android:id="@+id/txtMatriculaE"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtNomEf"
                android:layout_marginStart="30dp"
                android:layout_marginTop="25dp"
                android:text="183110356"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="145dp"
                android:layout_height="117dp"
                android:layout_below="@+id/textView7"
                android:layout_marginTop="10dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="30dp"
                app:srcCompat="@drawable/mavp" />

            <TextView
                android:id="@+id/txtMatriculaM"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtNomMa"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"
                android:text="183110618"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="145dp"
                android:layout_height="117dp"
                android:layout_below="@+id/txtMatriculaM"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                app:srcCompat="@drawable/kmr" />

            <TextView
                android:id="@+id/txtNomKa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView4"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:text="Karen Miliami Rodriguez Luz"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

            <TextView
                android:id="@+id/txtMatriculaK"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtNomKa"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="7dp"
                android:text="183110447"
                android:layout_marginBottom="20dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2" />
        </RelativeLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
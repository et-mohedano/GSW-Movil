<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <RelativeLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/txtTitulo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="152dp"
            android:layout_marginLeft="152dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="163dp"
            android:layout_marginRight="163dp"
            android:text="Mi Calculadora"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="16dp" />

        <TextView
            android:id="@+id/txtDato1"
            android:layout_width="53dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtTitulo"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="30dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="48dp"
            android:text="Dato 1:"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="85dp"
            />

        <EditText
            android:id="@+id/edtDato1"
            android:layout_width="184dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtTitulo"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="29dp"
            android:layout_marginLeft="29dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="113dp"
            android:layout_marginRight="113dp"
            android:layout_toEndOf="@+id/txtDato1"
            android:layout_toRightOf="@+id/txtDato1"
            android:ems="10"
            android:inputType="textPersonName"
            android:text=""
            tools:layout_editor_absoluteX="196dp"
            tools:layout_editor_absoluteY="85dp" />

        <TextView
            android:id="@+id/txtDato2"
            android:layout_width="53dp"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/txtDato1"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="28dp"
            android:layout_marginLeft="28dp"
            android:layout_marginBottom="-66dp"
            android:text="Dato 2:"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="140dp" />

        <EditText
            android:id="@+id/edtDato2"
            android:layout_width="185dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/edtDato1"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="29dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="114dp"
            android:layout_marginRight="114dp"
            android:layout_toEndOf="@+id/txtDato2"
            android:layout_toRightOf="@+id/txtDato2"
            android:ems="10"
            android:inputType="textPersonName"
            android:text=""
            tools:layout_editor_absoluteX="200dp"
            tools:layout_editor_absoluteY="140dp"
            tools:text="" />


        <Button
            android:id="@+id/btnSuma"
            android:layout_width="61dp"
            android:layout_height="47dp"
            android:layout_below="@+id/edtDato2"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="13dp"
            android:layout_marginLeft="13dp"
            android:layout_marginTop="20dp"
            android:text="+"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="190dp" />

        <Button
            android:id="@+id/btnResta"
            android:layout_width="61dp"
            android:layout_height="47dp"
            android:layout_below="@+id/edtDato2"
            android:layout_marginStart="18dp"
            android:layout_marginLeft="18dp"
            android:layout_marginTop="20dp"
            android:layout_toEndOf="@+id/btnSuma"
            android:layout_toRightOf="@+id/btnSuma"
            android:text="-"
            tools:layout_editor_absoluteX="93dp"
            tools:layout_editor_absoluteY="190dp" />

        <Button
            android:id="@+id/btnMultiplicacion"
            android:layout_width="61dp"
            android:layout_height="47dp"
            android:layout_below="@+id/edtDato2"
            android:layout_marginStart="18dp"
            android:layout_marginLeft="18dp"
            android:layout_marginTop="20dp"
            android:layout_toEndOf="@+id/btnResta"
            android:layout_toRightOf="@+id/btnResta"
            android:text="X"
            tools:layout_editor_absoluteX="171dp"
            tools:layout_editor_absoluteY="190dp" />

        <Button
            android:id="@+id/btnDivision"
            android:layout_width="61dp"
            android:layout_height="47dp"
            android:layout_below="@+id/edtDato2"
            android:layout_marginStart="18dp"
            android:layout_marginLeft="14dp"
            android:layout_marginTop="20dp"
            android:layout_toEndOf="@+id/btnMultiplicacion"
            android:layout_toRightOf="@+id/btnMultiplicacion"
            android:text="/"
            tools:layout_editor_absoluteX="250dp"
            tools:layout_editor_absoluteY="190dp" />

        <Button
            android:id="@+id/btnCalculo"
            android:layout_width="59dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/edtDato2"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="18dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="22dp"
            android:layout_marginRight="22dp"
            android:layout_toEndOf="@+id/btnDivision"
            android:layout_toRightOf="@+id/btnDivision"
            android:text="="
            tools:layout_editor_absoluteX="325dp"
            tools:layout_editor_absoluteY="190dp" />

        <Button
            android:id="@+id/btnClean"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btnMultiplicacion"
            android:layout_marginStart="101dp"
            android:layout_marginLeft="101dp"
            android:layout_marginTop="20dp"
            android:layout_toEndOf="@+id/btnPotencia"
            android:layout_toRightOf="@+id/btnPotencia"
            android:text="Limpiar" />

        <Button
            android:id="@+id/btnPotencia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btnMultiplicacion"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="66dp"
            android:layout_marginLeft="66dp"
            android:layout_marginTop="20dp"
            android:text="X^Y" />

        <Button
            android:id="@+id/btnSiguienteMain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btnPotencia"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="149dp"
            android:layout_marginLeft="149dp"
            android:layout_marginTop="26dp"
            android:onClick="siguiente"
            android:text="Siguiente" />

        <Button
            android:id="@+id/btnAcceso"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btnSiguienteMain"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="155dp"
            android:layout_marginLeft="155dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="160dp"
            android:layout_marginRight="160dp"
            android:onClick="acceso"
            android:text="Acceso" />

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>